[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty='%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[commit]
	template = ~/.git_message_template
[core]
	excludesfile = ~/.gitignore_global
	whitespace = fix,-indent-with-non-tab,trailing-space
	editor = code --wait
[push]
	default = simple
[help]
	autocorrect = 15
[color]
	ui = auto
[color "branch"] 
	current = yellow bold
	local = green bold
	remote = cyan bold
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold
